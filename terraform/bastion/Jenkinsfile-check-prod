pipeline {
  agent {
    label "built-in"
  }
  stages {
    stage("Run the bastion check for prod") {
      steps {
        script {
          build(
              job: "TDR Bastion Check",
              parameters: [
                  string(name: "STAGE", value: "prod"),
              ],
              wait: false)
        }
      }
    }
  }
}

